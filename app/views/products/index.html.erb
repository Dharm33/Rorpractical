<table class="table table-condensed">

<thead>
  <tr>
    <th>Name</th>
    <th>Price</th>
    <th>Quantity</th>>
    <th colspan="3"></th>
  </tr>
 </thead>


<% @product.each do |product| %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.price %></td>
    <td><%= product.quantity %></td>
    <td><%= product.product_image %></td>
  </tr>
<% end %>
</table>